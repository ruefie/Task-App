{
  "short_name": "TaskApp",
  "name": "My Task Calendar",
  "icons": [
    {
      "src": "/notification-badge.png",
      "sizes": "32x32",
      "type": "image/png"
    },
    {
      "src": "/notification-icon.png",
      "sizes": "256x256",
      "type": "image/png"
    }
