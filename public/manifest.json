    {
      "src": "/screenshots/screenshot-wide.png",
      "sizes": "2552x1464",
      "type": "image/png",
      "form_factor": "wide"          
    },
    {
      "src": "/screenshots/screenshot-mobile.png",
      "sizes": "438x940",
      "type": "image/png"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+taskapp",
      "url": "/?url=%s"
    }
  ]
